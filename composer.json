{
    "name": "rogerionascimento/santander-pay-slip",
    "description": "Biblioteca para geração de boletos através da api do Santander",
    "version": "0.0.1",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "SantanderPaySlip\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SantanderPaySlip\\SantanderPaySlipServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "Rogério Nascimento"            
        }
    ],
    "require": {
        "php": ">=8.2",
        "guzzlehttp/guzzle": "^7.9",
        "illuminate/cache": "^11.36",
        "illuminate/console": "^11.36"
    },    
    "require-dev": {
        "phpunit/phpunit": "^11.5"
    }
}
